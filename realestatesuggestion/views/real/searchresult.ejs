<h1 style="font-size: 20px;">&#160;&#160;Search Results:</h1>
<div style="margin:15px;">

<label>Suggested price: <%= result.suggestedPrice%> â‚¬</label><br>
<label>Area: <%= result.locatedArea%></label><br>
<label>Land Type: <%= result.landType%></label><br>
<label>Number of items used in search: <%= realEstates.length%></label><br>

</div>
<table class="table" style="margin:15px;">
  <tr>
    <!--<th>Symbol</th>-->
    <!--<th>Number of Shares</th>-->
    <th>Size</th>
    <th>Price</th>
    <th>Area</th>
    <th>Show on map</th>
  </tr>
  <% _.each(realEstates, function (realEstate) { %>
  <tr data-id="<%= realEstate.id %>" data-model="realEstate">
    <td><%= realEstate.size %></td>
    <td><%= realEstate.price %></td>
    <td><%= realEstate.area %></td>
    <td><a href="/mapView?id=<%= realEstate.id %>">show</a></td>
  </tr>
  <% })%>
</table>
