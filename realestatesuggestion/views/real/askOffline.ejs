<link href="/assets/css/examples.css" rel="stylesheet"/>
<div class="home-background">
  <div class="filter-black"></div>
  <div class="container">
    <div class="row-changeData">
      <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3 col-xs-10 col-xs-offset-1">
        <div id="askOffline" class="changeData-card">
          <h3 class="title">Your Information</h3>
          <div id="askOffline">
            <div class="alert alert-danger" id="error"></div>
            <div class="alert alert-success" id="success"></div>
            <form class="askOffline-form" role="form" action="/supportQuestion" method="post">
              <table style="width:100%;">
                <tr>
                  <td><label>Title</label></td>
                </tr>
                <tr>
                  <td><input type="text" class="form-control" placeholder="Title" name="qTitle"></td>
                </tr>
                <tr>
                  <td><label>Body</label></td>
                </tr>
                <tr>
                  <td><input type="text" class="form-control" placeholder="Body" name="qBody"></td>
                </tr>
                <tr>
                  <td>
                    <button name="submitQuestion" class="btn btn-fill btn-danger btn-block">Submit Question</button>
                    <input type="hidden" value="<%= user.id%>" name="userID">
                  </td>
                </tr>
              </table>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
